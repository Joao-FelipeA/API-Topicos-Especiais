generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Servicos{

  id Int @id @default(autoincrement())
  dta_abertura DateTime
  dta_conclusao DateTime
  status String
  valor_total Float

  @@map("servicos")
}

model Funcionario {
  id             Int      @id @default(autoincrement())
  nome           String
  telefone       Int
  email          String
  especialidade  String

  @@map("funcionarios")
}

